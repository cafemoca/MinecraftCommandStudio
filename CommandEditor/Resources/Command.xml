<?xml version="1.0" encoding="utf-8" ?>
<CommandDefinition>

  <Command name="/achievement">
    <Arg type="key"
         key="give">
      <Arg type="keywords">
        <Word>achievement</Word>
        <Word>stat</Word>
        <Arg type="target" />
      </Arg>
    </Arg>
  </Command>

  <Command name="/blockdata">
    <Arg type="location">
      <Arg type="location">
        <Arg type="location">
          <Arg type="tag" />
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/clear">
    <Arg type="target">
      <Arg type="item">
        <Arg type="numeric">
          <Arg type="numeric">
            <Arg type="tag" />
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/clone">
    <Arg type="location">
      <Arg type="location">
        <Arg type="location">
          <Arg type="location">
            <Arg type="location">
              <Arg type="location">
                <Arg type="location">
                  <Arg type="location">
                    <Arg type="location">
                      <Arg type="keywords">
                        <Word>filtered</Word>
                        <Word>masked</Word>
                        <Word>replace</Word>
                        <Arg type="keywords">
                          <Word>force</Word>
                          <Word>move</Word>
                          <Word>normal</Word>
                          <Arg type="block" />
                        </Arg>
                      </Arg>
                    </Arg>
                  </Arg>
                </Arg>
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/debug">
    <Arg type="keywords">
      <Word>start</Word>
      <Word>stop</Word>
      <Word>chunk</Word>
      <Arg type="location">
        <Arg type="location">
          <Arg type="location" />
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/defaultgamemode">
    <Arg type="keywords">
      <Word>survival</Word>
      <Word>creative</Word>
      <Word>adventure</Word>
      <Word>spectator</Word>
    </Arg>
  </Command>

  <Command name="/difficulty">
    <Arg type="keywords">
      <Word>peaceful</Word>
      <Word>easy</Word>
      <Word>normal</Word>
      <Word>hard</Word>
    </Arg>
  </Command>

  <Command name="/effect">
    <Arg type="target">
      <Arg type="effect"
           default="true">
        <Arg type="numeric">
          <Arg type="numeric">
            <Arg type="boolean" />
          </Arg>
        </Arg>
      </Arg>
      <Arg type="key"
           key="clear" />
    </Arg>
  </Command>

  <Command name="/enchant">
    <Arg type="target">
      <Arg type="enchant">
        <Arg type="numeric" />
      </Arg>
    </Arg>
  </Command>

  <Command name="/entitydata">
    <Arg type="entity">
      <Arg type="tag" />
    </Arg>
  </Command>

  <Command name="/execute">
    <Arg type="entity">
      <Arg type="location">
        <Arg type="location">
          <Arg type="location">
            <Arg type="command"
                 default="true" />
            <Arg type="key"
                 key="detect">
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/fill">
    <Arg type="location">
      <Arg type="location">
        <Arg type="location">
          <Arg type="location">
            <Arg type="location">
              <Arg type="location">
                <Arg type="block">
                  <Arg type="numeric">
                    <Arg type="key"
                         key="destroy">
                      <Arg type="tag" />
                    </Arg>
                    <Arg type="key"
                         key="hollow">
                      <Arg type="tag" />
                    </Arg>
                    <Arg type="key"
                         key="keep">
                      <Arg type="tag" />
                    </Arg>
                    <Arg type="key"
                         key="outline">
                      <Arg type="tag" />
                    </Arg>
                    <Arg type="key"
                         key="replace">
                      <Arg type="block">
                        <Arg type="numeric" />
                      </Arg>
                    </Arg>
                  </Arg>
                </Arg>
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/gamemode">
    <Arg type="keywords">
      <Word>survival</Word>
      <Word>creative</Word>
      <Word>adventure</Word>
      <Word>spectator</Word>
      <Arg type="target" />
    </Arg>
  </Command>

  <Command name="/gamerule">
    <Arg type="keywords"
         default="true">
      <Word>commandBlockOutput</Word>
      <Word>doDaylightCycle</Word>
      <Word>doFireTick</Word>
      <Word>doMobLoot</Word>
      <Word>doMobSpawning</Word>
      <Word>doTileDrops</Word>
      <Word>keepInventory</Word>
      <Word>logAdminCommands</Word>
      <Word>mobGriefing</Word>
      <Word>naturalRegeneration</Word>
      <Word>randomTickSpeed</Word>
      <Word>reducedDebugInfo</Word>
      <Word>sendCommandFeedback</Word>
      <Word>showDeathMessages</Word>
      <Arg type="boolean" />
    </Arg>
    <Arg type="key"
         key="randomTickSpeed">
      <Arg type="numeric" />
    </Arg>
  </Command>
  
  <Command name="/give">
    <Arg type="target">
      <Arg type="item">
        <Arg type="numeric">
          <Arg type="numeric">
            <Arg type="tag" />
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/kill">
    <Arg type="target" />
  </Command>

  <Command name="/op">
    <Arg type="target" />
  </Command>

  <Command name="/particle">
    <Arg type="particle">
      <Arg type="location">
        <Arg type="location">
          <Arg type="location">
            <Arg type="numeric">
              <Arg type="numeric">
                <Arg type="numeric">
                  <Arg type="numeric">
                    <Arg type="numeric">
                      <Arg type="keywords">
                        <Word>force</Word>
                      </Arg>
                    </Arg>
                  </Arg>
                </Arg>
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/playsound">
    <Arg type="sound">
      <Arg type="target">
        <Arg type="location">
          <Arg type="location">
            <Arg type="location">
              <Arg type="numeric">
                <Arg type="numeric">
                  <Arg type="numeric" />
                </Arg>
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/replaceitem">
    <Arg type="key"
         key="block">
      <Arg type="location">
        <Arg type="location">
          <Arg type="location">
            <Arg type="slot">
              <Arg type="item">
                <Arg type="numeric">
                  <Arg type="numeric">
                    <Arg type="tag" />
                  </Arg>
                </Arg>
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
    <Arg type="key"
         key="entity">
      <Arg type="target">
        <Arg type="slot">
          <Arg type="item">
            <Arg type="numeric">
              <Arg type="numeric">
                <Arg type="tag" />
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/scoreboard">
    <Arg type="key"
         key="objectives">
      <Arg type="key"
           key="list" />
      <Arg type="key"
           key="add">
        <Arg type="score">
          <Arg type="keywords">
            <Word>dummy</Word>
            <Word>trigger</Word>
            <Word>deathCount</Word>
            <Word>playerKillCount</Word>
            <Word>totalKillCount</Word>
            <Word>health</Word>
            <Arg type="text" />
          </Arg>
        </Arg>
      </Arg>
      <Arg type="key"
           key="remove">
        <Arg type="score" />
      </Arg>
      <Arg type="key"
           key="setdisplay">
        <Arg type="keywords">
          <Word>list</Word>
          <Word>sidebar</Word>
          <Word>belowName</Word>
          <Arg type="score" />
        </Arg>
      </Arg>
    </Arg>
    <Arg type="key"
         key="players">
      <Arg type="key"
           key="list">
        <Arg type="target" />
      </Arg>
      <Arg type="key"
           key="set">
        <Arg type="target">
          <Arg type="score">
            <Arg type="numeric">
              <Arg type="tag" />
            </Arg>
          </Arg>
        </Arg>
      </Arg>
      <Arg type="key"
           key="add">
        <Arg type="target">
          <Arg type="score">
            <Arg type="numeric">
              <Arg type="tag" />
            </Arg>
          </Arg>
        </Arg>
      </Arg>
      <Arg type="key"
           key="remove">
        <Arg type="target">
          <Arg type="score">
            <Arg type="numeric">
              <Arg type="tag" />
            </Arg>
          </Arg>
        </Arg>
      </Arg>
      <Arg type="key"
           key="reset">
        <Arg type="target">
          <Arg type="score" />
        </Arg>
      </Arg>
      <Arg type="key"
           key="enable">
        <Arg type="target">
          <Arg type="score" />
        </Arg>
      </Arg>
      <Arg type="key"
           key="test">
        <Arg type="target">
          <Arg type="score">
            <Arg type="numeric">
              <Arg type="numeric" />
            </Arg>
          </Arg>
        </Arg>
      </Arg>
      <Arg type="key"
           key="operation">
        <Arg type="target">
          <Arg type="score">
            <Arg type="keywords">
              <Word tip="+=\n加算">
                +=
              </Word>
              <Word tip="">
                -=
              </Word>
              <Word tip="">
                *=
              </Word>
              <Word tip="">
                /=
              </Word>
              <Word tip="">
                %=
              </Word>
              <Word tip="">
                =
              </Word>
              <Word tip="">
                &lt;
              </Word>
              <Word tip="">
                &gt;
              </Word>
              <Word tip="">
                &gt;&lt;
              </Word>
              <Arg type="target">
                <Arg type="score" />
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
    <Arg type="key"
         key="teams">
      <Arg type="key"
           key="list">
        <Arg type="team" />
      </Arg>
      <Arg type="key"
           key="add">
        <Arg type="team">
          <Arg type="text" />
        </Arg>
      </Arg>
      <Arg type="key"
           key="remove">
        <Arg type="team" />
      </Arg>
      <Arg type="key"
           key="empty">
        <Arg type="team" />
      </Arg>
      <Arg type="key"
           key="join">
        <Arg type="team" />
      </Arg>
      <Arg type="key"
           key="leave">
        <Arg type="team" />
      </Arg>
      <Arg type="key"
           key="option">
        <Arg type="team">
          <Arg type="key"
               key="color">
            <Arg type="color">
            </Arg>
          </Arg>
          <Arg type="key"
               key="friendlyfire">
            <Arg type="boolean" />
          </Arg>
          <Arg type="key"
               key="seeFriendlyInvisibles">
            <Arg type="boolean" />
          </Arg>
          <Arg type="key"
               key="nametagVisibility">
            <Arg type="keywords">
              <Word>never</Word>
              <Word>hideForOtherTeams</Word>
              <Word>hideForOwnTeam</Word>
              <Word>always</Word>
            </Arg>
          </Arg>
          <Arg type="key"
               key="deathMessageVisibility">
            <Arg type="keywords">
              <Word>never</Word>
              <Word>hideForOtherTeams</Word>
              <Word>hideForOwnTeam</Word>
              <Word>always</Word>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/setblock">
    <Arg type="location">
      <Arg type="location">
        <Arg type="location">
          <Arg type="block">
            <Arg type="numeric">
              <Arg type="keywords">
                <Word>destroy</Word>
