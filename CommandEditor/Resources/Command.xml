          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

  <Command name="/stats">
    <Arg type="key"
         key="block">
      <Arg type="location">
        <Arg type="location">
          <Arg type="location">
            <Arg type="key"
                 key="clear">
              <Arg type="keywords">
                <Word>AffectedBlocks</Word>
                <Word>AffectedEntities</Word>
                <Word>AffectedItems</Word>
                <Word>QueryResult</Word>
                <Word>SuccessCount</Word>
              </Arg>
            </Arg>
            <Arg type="key"
                 key="set">
              <Arg type="keywords">
                <Word>AffectedBlocks</Word>
                <Word>AffectedEntities</Word>
                <Word>AffectedItems</Word>
                <Word>QueryResult</Word>
                <Word>SuccessCount</Word>
                <Arg type="target">
                  <Arg type="score" />
                </Arg>
              </Arg>
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
    <Arg type="key"
         key="entity">
      <Arg type="target">
        <Arg type="key"
             key="clear">
          <Arg type="keywords">
            <Word>AffectedBlocks</Word>
            <Word>AffectedEntities</Word>
            <Word>AffectedItems</Word>
            <Word>QueryResult</Word>
            <Word>SuccessCount</Word>
          </Arg>
        </Arg>
        <Arg type="key"
             key="set">
          <Arg type="keywords">
            <Word>AffectedBlocks</Word>
            <Word>AffectedEntities</Word>
            <Word>AffectedItems</Word>
            <Word>QueryResult</Word>
            <Word>SuccessCount</Word>
            <Arg type="target">
              <Arg type="score" />
            </Arg>
          </Arg>
        </Arg>
      </Arg>
    </Arg>
  </Command>

